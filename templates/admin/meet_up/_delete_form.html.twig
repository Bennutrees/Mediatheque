<form method="post" action="{{ path('meet_up_delete', {'id': meet_up.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet objet ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ meet_up.id) }}">
    <button type="btn" class="btn btn-danger">Supprimer</button>
</form>
